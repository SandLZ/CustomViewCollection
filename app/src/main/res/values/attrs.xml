<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomArrowImage">
        <attr name="fixRate" format="float" />
        <attr name="arrowSrc" format="integer" />
        <attr name="allowRotation" format="boolean" />
        <attr name="position" format="enum">
            <enum name="top_left" value="0"/>
            <enum name="top_right" value="1"/>
            <enum name="bottom_left" value="2"/>
            <enum name="bottom_right" value="3"/>
        </attr>
    </declare-styleable>
</resources>